class Doctor extends MedicalProfessional {
    private String specialization;
    private int yearsOfExperience;

    public Doctor(String id, String name, int age, String gender, String employeeId, String department, double salary, String specialization, int yearsOfExperience) {
        super(id, name, age, gender, employeeId, department, salary);
        this.specialization = specialization;
        this.yearsOfExperience = yearsOfExperience;
    }

    @Override
    public String toString() {
        return "Doctor: " + name + " | Specialization: " + specialization + " | Experience: " + yearsOfExperience + " years";
    }
}
