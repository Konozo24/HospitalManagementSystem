class Nurse extends MedicalProfessional {
    private String shift;
    private String assignedDoctor;

    public Nurse(String id, String name, int age, String gender, String employeeId, String department, double salary, String shift, String assignedDoctor) {
        super(id, name, age, gender, employeeId, department, salary);
        this.shift = shift;
        this.assignedDoctor = assignedDoctor;
    }

    public String toString() {
        return "Nurse: " + name + " | Shift: " + shift + " | Assigned Doctor: " + assignedDoctor;
    }
}
