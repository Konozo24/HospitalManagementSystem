public class Person {
    protected String id;
    protected String name;
    protected int age;
    protected String gender;

    // Constructor
    public Person(String id, String name, int age, String gender) {
        this.id = id;
        this.name = name;
        this.age = age;
        this.gender = gender;
    }

    // Getters
    public String getId() { 
        return this.id; 
    }
    public String getName() { 
        return name; 
    }
    public int getAge() { 
        return age; 
    }
    public String getGender() { 
        return gender; 
    }

}
